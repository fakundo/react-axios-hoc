{
  "name": "react-axios-hoc",
  "version": "2.0.1",
  "description": "Observable axios actions for React components",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "scripts": {
    "dev": "webpack-dev-server --progress --open --config webpack.config.development.js",
    "docs": "webpack --progress --config webpack.config.docs.js",
    "lint": "eslint src && eslint examples",
    "clean": "rimraf lib",
    "compile": "NODE_ENV=commonjs babel src -d lib",
    "build": "npm run clean && npm run compile && npm run docs",
    "prepublishOnly": "yarn run lint && yarn run build"
  },
  "keywords": [
    "react",
    "action",
    "axios"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fakundo/react-axios-hoc"
  },
  "author": "Roman Samoylov (https://github.com/fakundo)",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.3",
    "@babel/plugin-proposal-class-properties": "^7.3.3",
    "@babel/plugin-proposal-export-default-from": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "axios": "^0.18.0",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "clean-webpack-plugin": "^1.0.1",
    "eslint": "^5.14.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-react": "^7.12.4",
    "html-webpack-plugin": "^3.2.0",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "rimraf": "^2.6.3",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.2.1"
  },
  "dependencies": {
    "@babel/runtime": "^7.3.1",
    "lodash": "^4.17.11"
  },
  "peerDependencies": {
    "axios": "*",
    "react": "*"
  }
}
